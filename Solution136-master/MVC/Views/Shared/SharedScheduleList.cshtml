
@{
    ViewBag.Title = "Schedule List";

}
<h3>Schedule List Page</h3>
<div id="divSharedScheduleList">
    <table>
        <thead>
            <tr>
                <th>Schedule Id</th>
                <th>Course Id</th>
                <th>Year</th>
                <th>Quarter</th>
                <th>Session</th>
                <th>Schedule Day Id</th>
                <th>Schedule Time Id</th>
                <th>Instructor Id</th>
            </tr>
        </thead>
        <tbody data-bind="foreach: viewModel">
            <tr>
                <td data-bind="text: schedule_id"></td>
                <td data-bind="text: course_id"></td>
                <td data-bind="text: year"></td>
                <td data-bind="text: quarter"></td>
                <td data-bind="text: session"></td>
                <td data-bind="text: schedule_day_id"></td>
                <td data-bind="text: schedule_time_id"></td>
                <td data-bind="text: instructor_id"></td>
                <td>
                    <!-- Note, this is an example using knockout templating. $data is the keyword
                         to for each element of the viewModel array -->
                    <div data-bind="template: { name: 'action-template', data: $data}"></div>
                </td>
            </tr>
        </tbody>
    </table>
    <div data-bind="template: { name: 'action-template2', data: $data}"></div>
</div>


<script type="text/javascript">
    $(document).ready(function () {
        var scheduleViewModelObj = new ScheduleViewModel();
        scheduleViewModelObj.GetScheduleList();
    });
</script>

