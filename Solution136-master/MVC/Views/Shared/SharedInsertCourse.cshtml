@{
    ViewBag.Title = "Insert Course";
}
<h2>Insert Course</h2>
<div id="divSharedInsertCourse">
    Course Id : <input type="text" data-bind="value: $data.course_id" /><br />
    <br />
    Course Name : <input type="text" data-bind="value: $data.title" /><br />
    <br />
    Course Level : <input type="text" data-bind="value: $data.course_level" /><br />
    <br />
    Description : <input type="text" data-bind="value: $data.description" /><br />
    <br />
    Department Id: <input type="text" data-bind="value: $data.department_id" /><br />
    <br />
    <input type="button" data-bind="value: 'Insert', click: $data.add" style="margin-left:80px" />
    <br /><br />
    <div id="divMessage"></div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        var courseViewModelObj = new CourseViewModel();
        courseViewModelObj.Initialize();
    });

</script>
