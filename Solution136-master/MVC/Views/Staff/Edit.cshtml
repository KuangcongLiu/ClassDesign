@{
    ViewBag.Title = "Edit";
    var staff_id = ViewBag.staff_id;
}
<h2>Edit</h2>
<div id="divEditStaff">
    Email : <input type="text" data-bind="value: $data.email" /><br />
    <br />
    Password : <input type="text" data-bind="value: $data.password" /><br />
    <br />
    <input type="button" data-bind="value: 'Update', click: $data.update" style="margin-left:80px" />
    <br /><br />
    <div id="divMessage"></div>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        var staffViewModelObj = new StaffViewModel();
        staffViewModelObj.Load(@staff_id);
    });

</script>
