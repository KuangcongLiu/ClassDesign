@{
    ViewBag.Title = "Edit Schedule";
    var schedule_id = ViewBag.schedule_id;

}
<h2>Edit Schedule</h2>
<div id="divSharedEditSchedule">
    Schedule Id : <input type="text" data-bind="value: $data.schedule_id" /><br />
    <br />
    Course Id : <input type="text" data-bind="value: $data.course_id" /><br />
    <br />
    Year : <input type="text" data-bind="value: $data.year" /><br />
    <br />
    Quarter : <input type="text" data-bind="value: $data.quarter" /><br />
    <br />
    Session : <input type="text" data-bind="value: $data.session" /><br />
    <br />
    Schedule Day Id : <input type="text" data-bind="value: $data.schedule_day_id" /><br />
    <br />
    Schedule Time Id : <input type="text" data-bind="value: $data.schedule_day_id" /><br />
    <br />
    Instructor Id : <input type="text" data-bind="value: $data.instructor_id" /><br />
    <br />
    <input type="button" data-bind="value: 'Update', click: $data.update" style="margin-left:80px" />
    <br /><br />
    <div id="divMessage"></div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        var scheduleViewModelObj = new ScheduleViewModel();
        scheduleViewModelObj.Load(@schedule_id);
    });

</script>
